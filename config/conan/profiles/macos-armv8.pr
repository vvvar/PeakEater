[settings]
arch=x86_64
build_type=Release
compiler=apple-clang
compiler.cppstd=17
compiler.libcxx=libc++
compiler.version=14
os=Macos

[conf]
tools.cmake.cmaketoolchain:generator=Ninja
tools.build:cxxflags=["-arch x86_64", "-arch arm64", "-mmacosx-version-min=10.9"]

[options]
peakeater/*:signed=True